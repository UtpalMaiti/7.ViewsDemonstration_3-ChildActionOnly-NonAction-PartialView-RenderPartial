@model List<ViewsDemonstration_3.Models.Employee>

<h2>Employees List...</h2>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Location</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.Id</td>
                <td>@emp.Name</td>
                <td>@emp.Location</td>
                <td>@emp.Salary</td>
            </tr>
        }
    </tbody>
</table>